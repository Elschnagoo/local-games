{
  "name": "@elschnagoo/local-games",
  "description": "A library to get local games from different platforms and launcher",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "build": "tsc",
    "buildprep": "npm run build",
    "pack": "npm run build && npm pack",
    "dev": "ts-node src/dev.ts",
